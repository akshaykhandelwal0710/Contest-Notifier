{
  "name": "Upcoming Contests",
  "manifest_version": 3,
  "description": "Get information about upcoming contests on various sites",
  "version": "1.0",
  "host_permissions": [
    "http://127.0.0.1:5000/*"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "static/popup.html",
    "default_icon": "icons/icon16.png"
  }
}